<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SeeStudentListController</servlet-name>
  	<display-name>SeeStudentListController</display-name>
  	<description></description>
  	<servlet-class>com.dsi.web.SeeStudentListController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddStudentController</servlet-name>
  	<display-name>AddStudentController</display-name>
  	<description></description>
  	<servlet-class>com.dsi.web.AddStudentController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SeeStudentListController</servlet-name>
  	<url-pattern>/seeStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddStudentController</servlet-name>
  	<url-pattern>/addStudent</url-pattern>
  </servlet-mapping>
</web-app>
